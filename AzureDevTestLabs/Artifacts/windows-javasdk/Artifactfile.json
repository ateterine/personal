{
    "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2016-11-28/dtlArtifacts.json",
    "title": "Install Java SE 8 Development Kit via the Chocolatey installer",
    "publisher": "Microsoft",
    "description": "Installs the Java SE 8 Development Kit via the Chocolatey installer.  More information:  https://chocolatey.org/packages/jdk8 ", 
    "tags": [
        "Windows",
        "Java",
        "JDK"
    ],
    "iconUri": "https://upload.wikimedia.org/wikipedia/en/thumb/3/30/Java_programming_language_logo.svg/128px-Java_programming_language_logo.svg.png",
    "targetOsType": "Windows",
    "parameters": {
        "selectParam": {
          "type": "string",
          "displayName": "Choose version of Java Development Kit to install",
          "description": "Only a selection from the list of JDK versions is allowed",
          "defaultValue": "jdk10",
          "allowEmpty": false,
          "allowedValues": [
            "jdk10",
            "jdk8",
            "jdk7",
            "jdk6"
          ]
        }
    },
    "runCommand": {
    "commandToExecute": "[concat('powershell.exe -ExecutionPolicy bypass \"& ./artifact-main.ps1', ' -JDKVersion ''', parameters('JDKVersion'), '''\"')]"
    }
}